#include "new.ch"
#include "ACHOICE.ch"
#include "pass.ch"
MEMVAR m_platpath
Function pervnum(m_platpath)
LOCAL m_vnum:=1
NET USE (m_platpath+"nakl1") NEW ALIAS n1 READONLY
NET USE (m_platpath+"nakl2") NEW ALIAS n2 READONLY
NET USE ("perekod") NEW 
ZAP
INDEX ON v1 to ("perekod") 



RETURN .t.

